{
  "name": "elymus",
  "version": "0.1.5",
  "description": "Censorship resistance at highest level",
  "main": "main.js",
  "repository": "https://github.com/angrymouse/elymus",
  "author": "Angrymouse <nik@hns.is>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-dmg": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps",
    "app:dist": "electron-builder"
  },
  "dependencies": {
    "@electron-forge/maker-wix": "^6.0.0-beta.65",
    "@reforged/maker-appimage": "^1.2.1",
    "bns": "^0.15.0",
    "bs58": "^5.0.0",
    "cids": "^1.1.9",
    "daisyui": "^2.24.0",
    "electron-builder-squirrel-windows": "^23.4.0",
    "electron-forge-maker-nsis": "^23.4.0",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "fastify": "^4.5.2",
    "ipfs": "^0.63.5",
    "mime": "^3.0.0",
    "multiformats": "^9.7.1",
    "pino": "^8.4.2",
    "undici": "^5.10.0",
    "yauzl": "^2.10.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "ignore": [
          "^\\/src$",
          "^\\/out$"
        ],
        "icon": "build/icon.ico",
        "name": "Elymus"
      },
      "makers": [{
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "asar": false,
            "name": "Elymus",
            "title": "Elymus",
            "productName": "Elymus",
            "genericName": "Elymus",
            "icon": "./src/assets/logo-01.png",
            "categories": [
              "Utility"
            ]
          }
        },
        {
          "name": "@reforged/maker-appimage",
          "config": {
            "options": {
              "name": "Elymus",
              "productName": "Elymus",
              "genericName": "Elymus",
              "icon": "./src/assets/logo-01.png",
              "categories": [
                "Utility"
              ],
              "AppImageKitRelease": 12
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./src/assets/elymus-01.png",
            "format": "ULFO"
          }
        }
      ]
    }
  },
  "build": {
    "asar": false,
    "productName": "Elymus",
    "copyright": "Angrymouse",
    "appId": "dweb.elymus.repens",
    "mac": {
      "category": "utility"
    },
    "files": [
      "!out",
      "!dist",
      "!src"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}